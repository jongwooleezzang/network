<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@page import="java.util.ArrayList" %>    
<%-- 1.<%@ 디렉티브 %>  --%>   
<%-- 2.<%! 선언문(Declare)  %> --%>
<%-- 3.<% 자바명령문(Scriptlet)  %>--%>
<%-- 4.<%= 표현식(Expression)  %>--%>
<%!
	private String name;
	public String getName(){
		return this.name;
	}
	public String setName(){
		return name;
	}
%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<%
	String title ="메인페이지";
%>
<title><%= title %></title>
</head>
<body>
<%
	setName("김기태");
%>
<h2><%=getName() %>의 홈페지이</h2>
</body>
</html>